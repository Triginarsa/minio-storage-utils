{
    "name": "triginarsa/minio-storage-utils",
    "description": "A comprehensive MinIO storage service package for Laravel with file processing capabilities",
    "type": "library",
    "keywords": ["minio", "laravel", "storage", "s3", "file-upload", "image-processing"],
    "license": "MIT",
    "authors": [
        {
            "name": "Triginarsa",
            "email": "your-email@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "aws/aws-sdk-php": "^3.0",
        "league/flysystem": "^3.0",
        "league/flysystem-aws-s3-v3": "^3.0",
        "intervention/image": "^3.0",
        "illuminate/support": "^10.0|^11.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^8.0",
        "mockery/mockery": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Triginarsa\\MinioStorageUtils\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Triginarsa\\MinioStorageUtils\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Triginarsa\\MinioStorageUtils\\Laravel\\MinioStorageServiceProvider"
            ],
            "aliases": {
                "MinioStorage": "Triginarsa\\MinioStorageUtils\\Laravel\\Facades\\MinioStorage"
            }
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
